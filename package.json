{
  "name": "clickable-prototype",
  "version": "0.4.0",
  "description": "Lightweight React Framework for creating clickable Prototypes",
  "author": "pinussilvestrus",
  "license": "MIT",
  "repository": "pinussilvestrus/clickable-prototype",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "start:example": "cd example && npm run start",
    "prepare": "npm run build",
    "predeploy": "cd example && npm install && npm run build",
    "dev": "run-p start start:example"
  },
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-node-resolve": "^6.0.0",
    "@rollup/plugin-url": "^4.0.0",
    "@svgr/rollup": "^5.0.0",
    "babel-eslint": "^10.0.3",
    "cross-env": "^6.0.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.1.1",
    "npm-run-all": "^4.1.5",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts": "^3.3.0",
    "react-test-renderer": "^16.12.0",
    "rollup": "^1.27.14",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^2.0.3"
  },
  "files": [
    "dist"
  ]
}
